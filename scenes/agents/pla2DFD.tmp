script/source = "extends Node3D

var main_sm: LimboHSM
var player: CharacterBody3D

@export var ap: AnimationPlayer
@export var dust_effects: Node3D

func _ready():
	player = get_parent()	
	initiate_state_machine()

func initiate_state_machine():
	main_sm = LimboHSM.new()

	# Dictionary to store state instances
	var states = {}
	
	# Define a dictionary with state names and their paths
	var state_paths = {
		\"idle_state\": \"res://scripts/state_machine/states/idle_state.gd\",
		\"move_state\": \"res://scripts/state_machine/states/move_state.gd\",
		\"skid_state\": \"res://scripts/state_machine/states/skid_state.gd\",
		\"jump_state\": \"res://scripts/state_machine/states/jump_state.gd\",
		\"fall_state\": \"res://scripts/state_machine/states/fall_state.gd\",
		\"land_state\": \"res://scripts/state_machine/states/land_state.gd\",
		\"move_after_land_state\": \"res://scripts/state_machine/states/move_after_land_state.gd\",
		\"shield_in_state\": \"res://scripts/state_machine/states/shield_in_state.gd\",
		\"shield_out_state\": \"res://scripts/state_machine/states/shield_out_state.gd\",
		\"shield_in_air_state\": \"res://scripts/state_machine/states/shield_in_air_state.gd\",
		\"shield_tumble_state\": \"res://scripts/state_machine/states/shield_tumble_state.gd\",
		\"shield_land_state\": \"res://scripts/state_machine/states/shield_land_state.gd\",
		\"move_collide_state\": \"res://scripts/state_machine/states/move_collide_state.gd\"
	}
	
	# Instantiate and set up each state dynamically
	for state_name in state_paths.keys():
		var state_instance = load(state_paths[state_name]).new()
		state_instance.state_machine = self
		state_instance.player = player
		main_sm.add_child(state_instance)
		
		# Store the instance in the dictionary
		states[state_name] = state_instance
	
	# Set the initial state (assuming it's the idle state)
	main_sm.initial_state = main_sm.get_child(0)  # Sets the first added child as the initial state
	
	handle_transitions(states) #handle transitions
	
	# Initialize and activate the state machine
	main_sm.initialize(self)
	main_sm.set_active(true)

	# Add the state machine to the player
	add_child(main_sm)

func handle_transitions(states):
	main_sm.add_transition(states[\"idle_state\"], states[\"move_state\"], &\"to_move\")
	main_sm.add_transition(states[\"idle_state\"], states[\"move_collide_state\"], &\"to_move_collide\")
	main_sm.add_transition(states[\"idle_state\"], states[\"jump_state\"], &\"to_jump\")
	main_sm.add_transition(states[\"idle_state\"], states[\"shield_in_state\"], &\"to_shield_in\")

	main_sm.add_transition(states[\"move_state\"], states[\"idle_state\"], &\"to_idle\")
	main_sm.add_transition(states[\"move_state\"], states[\"skid_state\"], &\"to_skid\")
