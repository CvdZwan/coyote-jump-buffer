	main_sm.add_transition(states[\"move_state\"], states[\"fall_state\"], &\"to_fall\")
	main_sm.add_transition(states[\"move_state\"], states[\"shield_in_state\"], &\"to_shield_in\")
	main_sm.add_transition(states[\"move_state\"], states[\"move_collide_state\"], &\"to_move_collide\")
	
	main_sm.add_transition(states[\"move_collide_state\"], states[\"idle_state\"], &\"to_idle\")
	main_sm.add_transition(states[\"move_collide_state\"], states[\"skid_state\"], &\"to_skid\")
	main_sm.add_transition(states[\"move_collide_state\"], states[\"jump_state\"], &\"to_jump\")
	main_sm.add_transition(states[\"move_collide_state\"], states[\"fall_state\"], &\"to_fall\")
	main_sm.add_transition(states[\"move_collide_state\"], states[\"shield_in_state\"], &\"to_shield_in\")
	main_sm.add_transition(states[\"move_collide_state\"], states[\"move_state\"], &\"to_move\")
	
	main_sm.add_transition(states[\"skid_state\"], states[\"idle_state\"], &\"to_idle\")
	main_sm.add_transition(states[\"skid_state\"], states[\"move_state\"], &\"to_move\")
	main_sm.add_transition(states[\"skid_state\"], states[\"jump_state\"], &\"to_jump\")
	main_sm.add_transition(states[\"skid_state\"], states[\"fall_state\"], &\"to_fall\")
	main_sm.add_transition(states[\"skid_state\"], states[\"shield_in_state\"], &\"to_shield_in\")

	main_sm.add_transition(states[\"jump_state\"], states[\"fall_state\"], &\"to_fall\")
	main_sm.add_transition(states[\"jump_state\"], states[\"shield_in_air_state\"], &\"to_shield_in_air\")

	main_sm.add_transition(states[\"fall_state\"], states[\"idle_state\"], &\"to_idle\")
	main_sm.add_transition(states[\"fall_state\"], states[\"land_state\"], &\"to_land\")
	main_sm.add_transition(states[\"fall_state\"], states[\"jump_state\"], &\"to_jump\")
	main_sm.add_transition(states[\"fall_state\"], states[\"move_after_land_state\"], &\"to_move_after_land\")
	main_sm.add_transition(states[\"fall_state\"], states[\"shield_in_air_state\"], &\"to_shield_in_air\")
	
	main_sm.add_transition(states[\"land_state\"], states[\"idle_state\"], &\"to_idle\")
	main_sm.add_transition(states[\"land_state\"], states[\"move_state\"], &\"to_move\")
	main_sm.add_transition(states[\"land_state\"], states[\"jump_state\"], &\"to_jump\")
	main_sm.add_transition(states[\"land_state\"], states[\"skid_state\"], &\"to_skid\")
	main_sm.add_transition(states[\"land_state\"], states[\"shield_out_state\"], &\"to_shield_out\")
	
	
	main_sm.add_transition(states[\"move_after_land_state\"], states[\"move_state\"], &\"to_move\")
	main_sm.add_transition(states[\"move_after_land_state\"], states[\"jump_state\"], &\"to_jump\")
	main_sm.add_transition(states[\"move_after_land_state\"], states[\"shield_in_state\"], &\"to_shield_in\")
	main_sm.add_transition(states[\"move_after_land_state\"], states[\"skid_state\"], &\"to_skid\")
	
	
	main_sm.add_transition(states[\"shield_in_state\"], states[\"shield_out_state\"], &\"to_shield_out\")
	main_sm.add_transition(states[\"shield_in_state\"], states[\"shield_tumble_state\"], &\"to_shield_tumble\")
	
